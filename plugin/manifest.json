{
  "name": "Wefaves",
  "version": "0.1",
  "description": "Gestionnaire de session universel",
  "permissions": [
    "bookmarks",
    "unlimitedStorage",
    "tabs"
  ],
  "browser_action": {
      "default_title": "Wefaves",
      "default_popup": "popup.html"
  },
  "icons": {
      "48": "icon_48x48.png"
    },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
